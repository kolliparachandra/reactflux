{
  "extends": "react",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 2,
    "no-underscore-dangle": 2,
    "no-alert": 2,
    "no-lone-blocks": 2
  },
  "globals": {
     jQuery: true,
    $: true
  }
}